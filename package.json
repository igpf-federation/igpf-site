{
  "name": "igpf-site",
  "version": "1.2.1",
  "private": true,
  "eslintConfig": {
    "extends": "mcclowes"
  },
  "babel": {
    "plugins": [
      "babel-plugin-styled-components",
      "ramda"
    ]
  },
  "author": "Max Clayton Clowes <maxcc@me.com>",
  "main": "build/index.html",
  "dependencies": {
    "codogo-nav": "^2.4.1",
    "codogo-utility-functions": "^1.3.0",
    "eslint-loader": "^2.1.2",
    "imagesloaded": "^4.1.3",
    "marked": "^0.6.1",
    "masonry-layout": "^4.2.2",
    "mcclowes-scripts": "^0.10.5",
    "moment": "^2.24.0",
    "ramda": "^0.26.1",
    "react": "^15.6.1",
    "react-addons-transition-group": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-ga": "^2.5.7",
    "react-helmet": "^5.1.3",
    "react-responsive": "^3.0.0",
    "react-responsive-carousel": "^3.1.47",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "recompose": "^0.24.0",
    "slugify": "^1.3.4",
    "split": "^1.0.1",
    "styled-components": "^2.4.1",
    "velocity-animate": "^1.5.2"
  },
  "devDependencies": {
    "babel-plugin-ramda": "^2.0.0",
    "babel-plugin-styled-components": "^1.10.0",
    "contentful": "^7.5.0",
    "dotenv": "^7.0.0",
    "eslint": "^4.19.1",
    "eslint-config-mcclowes": "^0.6.2",
    "eslint-config-prettier": "^4.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.16.4",
    "process-promises": "^1.0.0",
    "prop-types": "^15.6.2",
    "react-snapshot": "^1.2.0",
    "sitemap-static": "^0.4.4",
    "webpack-cli": "^3.3.0"
  },
  "pre-commit": [
    "format"
  ],
  "scripts": {
    "start": "mcclowes-scripts start",
    "build": "mcclowes-scripts pack",
    "static": "npm run data && npm run build && react-snapshot && npm run sitemap",
    "data": "node scripts/static/makeDataFile",
    "format": "prettier --single-quote --trailing-comma=all --jsx-bracket-same-line=false --write '**/*.js'",
    "sitemap": "sitemap-static --prefix=https://www.islingtongp.org/ build | sed -e \"s/\\.html//g\" > build/sitemap.xml"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
